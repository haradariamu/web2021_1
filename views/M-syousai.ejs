<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Show message</title>
</head>
<body>
  <h2>メギド詳細</h2>
  <table border="1">
      <% for( let row of data ) { %>

        <tr>
            <td>スタイル</td>
            <td>クラス</td>
        </tr>
        <tr>
            <td><%= row.スタイル %></td>
            <td><%= row.クラス %></td>
        </tr>
        <tr>
            <td>名前</td>
            <td>HP</td>
            <td>攻撃力</td>
            <td>防御力</td>
            <td>素早さ</td>
        </tr>
        <tr>
            <td><%= row.名前 %></td>
            <td><%= row.HP %></td>
            <td><%= row.攻撃力 %></td>
            <td><%= row.防御力 %></td>
            <td><%= row.素早さ %></td>
        </tr>
      <% } %>
  </table>
</body>
</html>