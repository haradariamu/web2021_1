<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Show message</title>
</head>
<body>
  <h2>競馬場トップページ</h2>
  <table keibajyou="1">
    <% for (let row of data) {%>
      <tr>
        <tb><%= row.id %></tb>
        <td><a herf="/rece/<%= row.id %>"><%= row.name %></a></td>
      </tr>
    <% } %>
  </table><br>
  <input type="button" onclick="history.back()" value="戻る">
</body>
</html>