<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Show message</title>
</head>
<body>
  <h2>メギド一覧</h2>
  <table>
      <% for( let row of data ) { %>
        <tr>
            <td><%= row.名前 %></td>
            <td><%= row.HP %></td>
            <td><%= row.攻撃力 %></td>
            <td><%= row.防御力 %></td>
            <td><%= row.素早さ %></td>
            
        </tr>
      <% } %>
  </table>
</body>
</html>